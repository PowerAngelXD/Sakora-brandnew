let a:string;
a = "who";
if (a == "hello world") {
    a = "hello";
}
else if (a == "who") {
    a = "world";
}

match (a) {
    "world!"=> {
        a = "woRld";
    }
    "hello"=> {
        a = "heLLo";
    }
    default=> {
        a = "hello world";
    }
}

let i = 0;
while (i < 10) {
    a = a + "world!";
    i = i + 1;
}