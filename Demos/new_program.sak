let a:string = "who";
let i = 0;
while (i < 20) {
    match (a) {
        "who" => {
            a = "whowho";
        }
        "whowho" => {
            a = "hello";
        }
        "hello" => {
            a = a + "world";
        }
        "hello world" => {
            a = a + "what tf";
        }
        default => {
            a = a + "jjj";
        }
    }

    if (i > 10) {
        i = i + 5;
    }
    i = i + 1;
}