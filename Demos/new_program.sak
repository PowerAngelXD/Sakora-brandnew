let a:string = "who";
let i = 0;
while (i < 20000) {
    match (a) {
        "who" => {
            a = "whowho";
        }
        "whowho" => {
            a = "hello";
        }
        "hello" => {
            a += "world";
        }
        "hello world" => {
            a += "what tf";
        }
        default => {
            a += " [X] ";
        }
    }

    if (i > 10) {
        i += 5;
    }
    i ++;
}